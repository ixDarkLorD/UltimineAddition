{
  "schemaVersion": 1,
  "id": "${modid}",
  "version": "${mod_version}",

  "name": "${mod_name}",
  "description": "${mod_description}",
  "authors": [
    "${mod_author}"
  ],
  "contact": {
    "homepage": "${mod_page_url}",
    "issues": "${mod_issue_url}",
    "sources": "${mod_source_url}"
  },

  "license": "${mod_license}",
  "icon": "mod_logo.png",

  "environment": "${mod_environment}",
  "entrypoints": {
    "main": ["net.ixdarklord.ultimine_addition.core.fabric.FabricSetup"],
    "client": ["net.ixdarklord.ultimine_addition.core.fabric.FabricClientSetup"],
    "fabric-datagen": ["net.ixdarklord.ultimine_addition.datagen.DataGeneration"],
    "jei_mod_plugin": ["net.ixdarklord.ultimine_addition.integration.jei.JEIIntegration"]
  },
  "mixins": [
    "${modid}.mixins.json",
    "${modid}.common.mixins.json"
  ],
  "accessWidener" : "${modid}.accesswidener",

  "depends": {
    "java": ">=17",
    "minecraft": ">=${mc_version} <=${max_mc_version}",
    "fabricloader": ">=${min_fabric_version}",
    "fabric-api": ">=${min_fabric_api_version}",
    "forgeconfigapiport": ">=${versions.forgeconfigapiport.get()}",
    "architectury": ">=${versions.architectury.get()}",
    "coolcatlib": ">=${coolcatlib_version}",
    "ftbultimine": ">=${ftbultimine_version}"
  },
  "suggests": {
    "jei": ">=${versions.jei.get()}",
    "trinkets": ">=${versions.trinkets.get()}"
  }
}